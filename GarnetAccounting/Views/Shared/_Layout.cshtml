        string userRole = "نامشخص";
        var userSett = await _gs.GetUserSettingAsync(User.Identity.Name);
        string ActiveSeller = userSett?.ActiveSellerName;
        string ActivePeriod = userSett?.ActiveSellerPeriodName;
        string PeriodLabel = "دوره مالی فعالی وجود ندارد";
        bool hasPeriod = false;
        var periodData = new GarnetAccounting.Areas.Accounting.Models.Entities.Acc_FinancialPeriod()
            {
                DefualtVatRate = 10,
                StartDate = DateTime.Now,
                EndDate = DateTime.Now,
            };
        if (userSett != null && userSett.ActiveSellerPeriod.HasValue)
        {
            periodData = await _gs.GetFinancialPeriodAsync(userSett.ActiveSellerPeriod ?? 0);

        }

        if (!string.IsNullOrEmpty(ActivePeriod))
        {
            PeriodLabel = " دوره مالی " + ActivePeriod;
            hasPeriod = true;
        }
        var userPermisions = await _gs.UserSettingAsync(User.Identity.Name);
        if (_sign.IsSignedIn(User))
        {
            user = _usermanager.UserInfo(User.Identity.Name);
            userFullName = user.FirstName + " " + user.LastName;

            if (string.IsNullOrEmpty(user.Image))
                avatarSrc = user.Gender == 1 ? "../../img/avatars/1.png" : "../../img/avatars/2.png";
            else
                avatarSrc = "../../img/avatars/" + user.Image;

            var roles = await _usermanager.GetUserRolesVmAsync(user.UserName);
            if (roles.Count > 0)
                userRole = roles.Any(n => n.RoleName.Contains("Admin")) ? "مدیر" : roles.FirstOrDefault().RoleDescription;
